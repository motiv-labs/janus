{
    "name": "My First Proxy",
    "slug": "my_first_proxy",
    "active": true,
    "allowed_ips": [],
    "domain": "",
    "proxy": {
        "check_host_against_uptime_tests": false,
        "enable_load_balancing": false,
        "listen_path": "/posts/*randomName",
        "methods": [
            "GET"
        ],
        "preserve_host_header": false,
        "strip_listen_path": true,
        "append_listen_path": false,
        "target_list": [],
        "target_url": "https://jsonplaceholder.typicode.com/posts"
    },
    "rate_limit": {
        "enabled": true,
        "limit": "10-S"
    },
    "use_basic_auth": false,
    "use_oauth2": false,
    "cors_meta": {
        "domains": [
            "*"
        ],
        "enabled": true,
        "exposed_headers": [
            "X-Debug-Token",
            "X-Debug-Token-Link"
        ],
        "methods": [
            "GET",
            "POST",
            "PUT",
            "PATCH",
            "DELETE"
        ],
        "request_headers": [
            "Origin",
            "Authorization",
            "Content-Type"
        ]
    }
}